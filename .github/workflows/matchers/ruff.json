{
    "problemMatcher": [
      {
        "owner": "ruff-error",
        "severity": "error",
        "pattern": [
          {
            "regexp": "^(.*):(\\d+):(\\d+):\\s([EF]\\d{3}\\s.*)$",
            "file": 1,
            "line": 2,
            "column": 3,
            "message": 4
          }
        ]
      },
      {
        "owner": "ruff-warning",
        "severity": "warning",
        "pattern": [
          {
            "regexp": "^(.*):(\\d+):(\\d+):\\s([CDNW]\\d{3}\\s.*)$",
            "file": 1,
            "line": 2,
            "column": 3,
            "message": 4
          }
        ]
      }
    ]
  }